  "repository": {
    "type": "git",
    "url": "git+https://github.com/btims512/storybook-library.git"
  },
  "author": "Ben Tims",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btims512/storybook-library/issues"
  },
  "homepage": "https://github.com/btims512/storybook-library#readme",
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/mdx": "^3.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
